@using TravelClient.mvc.Models;
@model List<City>;
<h1>Cities</h1>


<ol>
@foreach (City city in @Model)
{
    <ul>
        <li>
            <a href="/reviews/@city.CityId">
                @city.CityName @city.Rating @city.ReviewCount
            </a>
        </li>
        <li>
            @Html.ActionLink("link", "Index", "Reviews", new {id = city.CityId})
        </li>
    </ul>
    <br>
}
</ol>